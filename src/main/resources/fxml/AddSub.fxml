<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0"
	prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.felixkroemer.iliasfilemanager.control.AddSubController">
	<children>
		<AnchorPane VBox.vgrow="ALWAYS">
			<children>
				<TreeView fx:id="treeView" prefHeight="275.0"
					prefWidth="500.0" AnchorPane.bottomAnchor="25.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0" />
				<AnchorPane layoutY="273.0" minHeight="0.0"
					minWidth="0.0" prefHeight="25.0" prefWidth="500.0"
					AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
					AnchorPane.rightAnchor="0.0">
					<children>
						<Pane layoutY="260.0" prefHeight="25.0" prefWidth="500.0"
							AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
							AnchorPane.rightAnchor="0.0">
							<children>
								<Button fx:id="addButton" layoutX="11.0"
									mnemonicParsing="false" onAction="#addSubscription"
									prefHeight="25.0" prefWidth="52.0" text="Add" />
								<Text fx:id="statusText" layoutX="100.0" layoutY="17.0"
									strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER"
									wrappingWidth="350.0" />
							</children>
						</Pane>
					</children>
				</AnchorPane>
			</children>
		</AnchorPane>
	</children>
</VBox>
